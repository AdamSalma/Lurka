{
  "plugins": [
    "react-hot-loader/babel",
    "transform-decorators-legacy",
    "transform-async-to-generator",
    ["module-resolver", {
      "alias": {
        "-": "./",
        "~": "./src/UI",
        "sass": "./src/UI/sass/",
        "public": "./src/Server/public"
      }
    }]
  ],
  "presets": [
    "es2015",
    "react",
    "stage-1",
    "flow"
  ]
}
