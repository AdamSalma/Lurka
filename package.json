{
  "author": "Adam Salma <adam3salma@gmail.com>",
  "name": "Lurka",
  "version": "0.0.6",
  "description": "Messaging board viewer and scraper.",
  "node-main": "./server",
  "main": "http://localhost:3000",
  "node-remote": "localhost",
  "scripts": {
    "dev": "node ./server",
    "start": "cross-env NODE_ENV=production ./node_modules/.bin/nw ./server/index.js",
    "build": "rimraf dist && cross-env NODE_ENV=production webpack --config ./webpack.production.config.js --progress --profile --colors",
    "test": "cross-env NODE_ENV=test ./node_modules/.bin/mocha",
    "test-watch": "cross-env NODE_ENV=test ./node_modules/.bin/mocha -R min -w"
  },
  "window": {
    "toolbar": true,
    "width": 1200,
    "height": 800
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AdamSalma/Lurka.git"
  },
  "keywords": [
    "React",
    "Webpack",
    "Babel",
    "Memes",
    "Viewer"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/AdamSalma/Lurka/issues"
  },
  "homepage": "https://github.com/AdamSalma/Lurka#readme",
  "devDependencies": {
    "babel-preset-es2015": "^6.14.0",
    "cross-env": "^3.0.0",
    "eslint": "^3.5.0",
    "eslint-plugin-import": "^1.15.0",
    "eslint-plugin-react": "^6.2.1",
    "expect": "1.20.2",
    "expect-jsx": "2.6.0",
    "mocha": "^3.1.0",
    "nodemon": "^1.10.2",
    "postcss-loader": "^0.13.0",
    "react-addons-test-utils": "^15.3.1",
    "react-hot-loader": "3.0.0-beta.2",
    "rimraf": "^2.5.4",
    "webpack-dev-middleware": "^1.7.0",
    "babel-loader": "^6.2.5",
    "css-loader": "^0.25.0",
    "file-loader": "0.9.0",
    "jsx-loader": "^0.13.2",
    "sass-loader": "^4.0.2",
    "style-loader": "0.13.1",
    "url-loader": "0.5.7",
    "autoprefixer": "^6.4.1",
    "webpack-hot-middleware": "2.12.2",
    "webpack": "^1.13.2"
  },
  "dependencies": {
    "assets-webpack-plugin": "3.4.0",
