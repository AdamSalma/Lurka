{
  "author": "Adam Salma <adam3salma@gmail.com>",
  "name": "Lurka",
  "version": "0.15.0",
  "description": "A 4chan desktop app for power users",
  "scripts": {
    "build": "gulp build",
    "start": "electron ./app",
    "test": "cross-env NODE_ENV=test NODE_PATH=./app/node_modules mocha || exit 0",
    "dev": "concurrently --kill-others \"npm run server\" \"npm run electron-window\"",
    "server": "cross-env NODE_PATH=./app/node_modules nodemon --watch ./src/Server ./src/Server",
    "electron-window": "electron ./src/Electron/dev",
    "flow": "flow",
    "clear": "gulp clear",
    "bundle:ui": "gulp bundle:ui",
    "bundle:server": "gulp bundle:server",
    "test:watch": "cross-env NODE_ENV=test NODE_PATH=./app/node_modules mocha -R min -w || exit 0",
    "uninstall": "rimraf ./node_modules && rimraf ./app/node_modules",
    "reinstall": "npm run uninstall && npm install",
    "postinstall": "cd ./app && npm install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AdamSalma/Lurka.git"
  },
  "keywords": [
    "React",
    "Webpack",
    "Babel",
    "Memes",
    "Viewer"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/AdamSalma/Lurka/issues"
  },
  "homepage": "https://github.com/AdamSalma/Lurka#readme",
  "devDependencies": {
    "autoprefixer": "^7.1.2",
    "axios-mock-adapter": "^1.9.0",
    "babel-loader": "^7.1.1",
    "babel-plugin-module-resolver": "^2.7.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-polyfill": "^6.16.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-1": "^6.16.0",
    "babel-register": "^6.16.3",
    "cli-color": "^1.1.0",
    "concurrently": "^3.5.0",
    "copy-webpack-plugin": "^4.0.1",
    "cross-env": "^5.0.1",
    "css-loader": "^0.28.4",
    "del": "^3.0.0",
    "devtool": "^2.3.0",
    "electron": "^1.6.11",
    "errorhandler": "^1.5.0",
    "eslint": "^4.1.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-react": "^7.1.0",
    "expect": "1.20.2",
    "expect-jsx": "^3.0.0",
    "file-loader": "^0.11.2",
    "flow-bin": "^0.49.1",
    "gulp": "^3.9.1",
    "html-webpack-plugin": "^2.29.0",
    "ignore-loader": "^0.1.1",
    "json-loader": "^0.5.4",
    "jsx-loader": "^0.13.2",
    "mocha": "^3.1.0",
    "node-sass": "^4.5.0",
    "nodemon": "^1.10.2",
    "postcss-loader": "^1.3.2",
    "postcss-nested": "^2.0.2",
    "react-addons-test-utils": "^15.6.0",
    "react-hot-loader": "3.0.0-beta.2",
    "rimraf": "^2.5.4",
    "run-sequence": "^2.0.0",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.18.2",
    "url-loader": "^0.5.9",
    "webpack": "^3.1.0",
    "webpack-dev-middleware": "^1.11.0",
    "webpack-hot-middleware": "^2.18.2"
  },
  "dependencies": {}
}
