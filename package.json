    "express": "4.14.0",
    "extract-text-webpack-plugin": "1.0.1",
    "morgan": "1.7.0",
    "node-sass": "^3.10.0",
    "nw": "^0.17.4",
    "react": "^15.3.1",
    "react-dom": "^15.3.1",
    "react-redux": "4.4.5",
    "react-router": "^2.8.1",
    "react-timeago": "^3.1.3",
    "redux": "^3.6.0",
    "redux-logger": "^2.6.1",
    "redux-thunk": "^2.1.0",
